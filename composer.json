{
    "name": "mlsolutions/nova-phone-field",
    "description": "Nova Phone Number field with a dynamic mask based on the country code inserted by the user.",
    "keywords": [
        "ml-solutions-ltda",
        "laravel",
        "nova",
        "phone mask",
        "input mask"
    ],
    "homepage": "https://github.com/MlSolutions/nova-phone-field",
    "license": "MIT",
    "authors": [],
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "laravel/nova": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "MlSolutions\\NovaPhoneField\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MlSolutions\\NovaPhoneField\\FieldServiceProvider"
            ]
        }
    },
    "scripts": {
        "lint": "pint"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "laravel/pint": "^1.10"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.mlsolutions.net.br"
        }
    ]
}
